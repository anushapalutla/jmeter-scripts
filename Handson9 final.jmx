              <stringProp name="script">BufferedWriter bw=new BufferedWriter(new FileWriter(&quot;Task9-2.csv&quot;,true));
bw.write(&quot;doctorName,specialiation,consultation fee, appointment date&quot;+&quot;\n&quot;);

int n=Integer.valueOf(vars.get(&quot;c_price_matchNr&quot;));
int sumPrice=0;
for(int i=n;i&gt;n-4;i--)
{
	String spec=vars.get(&quot;c_specialization_&quot;+i);
	String name=vars.get(&quot;c_doctorname_&quot;+i);
	String price=vars.get(&quot;c_price_&quot;+i);
	String date=vars.get(&quot;c_date_&quot;+i);
	bw.write(spec+&quot;,&quot;+name+&quot;,&quot;+price+&quot;,&quot;+date+&quot;\n&quot;);
	sumPrice+=Integer.valueOf(price);
}
bw.write(&quot;total consulation fee:&quot;+sumPrice+&quot;\n&quot;);
bw.close();
</stringProp>
