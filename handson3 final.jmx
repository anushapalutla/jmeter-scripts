int n=Integer.valueOf(vars.get(&quot;C_Searchres_matchNr&quot;));
bw.write(&quot;\n&quot;+&quot;search results after sorting and filtering:&quot;);
for(int i=1;i&lt;=n;i++)
{      
	String str=vars.get(&quot;C_Searchres_&quot;+i);       
	bw.write(&quot;\n&quot;+str);       
}
String p1=vars.get(&quot;C_Price1&quot;);
String p2=vars.get(&quot;C_Price2&quot;);
p1=p1.replace(&quot;,&quot;,&quot;&quot;);
p2=p2.replace(&quot;,&quot;,&quot;&quot;);
double price1=Double.valueOf(p1);
double price2=Double.valueOf(p2);
price1=price1*72;
price2=price2*72;
bw.write(&quot;\n&quot;+&quot;product1 name:&quot;+&quot;${C_Pname1}&quot;+&quot;\n&quot;+&quot;Product1 price:&quot;+price1+&quot;INR&quot;+&quot;\n&quot;+&quot;product1 qty:&quot;+&quot;${C_Quty1}&quot;+&quot;\n&quot;+&quot;sort1:&quot;+&quot;${c_sort}&quot;);
bw.write(&quot;\n&quot;+&quot;product2 name:&quot;+&quot;${C_Pname2}&quot;+&quot;\n&quot;+&quot;Product2 price:&quot;+price2+&quot;INR&quot;+&quot;\n&quot;+&quot;product2 qty:&quot;+&quot;${C_Quty2}&quot;+&quot;\n&quot;+&quot;sort2:&quot;+&quot;${c_sort}&quot;);

bw.write(&quot;\n&quot;);
bw.close();</stringProp>
