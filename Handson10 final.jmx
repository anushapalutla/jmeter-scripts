          <stringProp name="script">String ProductList=&quot;MobilePhone,Laptop,Television,Refrigerator,AirCooler&quot;;
String[]products=ProductList.split(&quot;,&quot;);
int random=new java.util.Random().nextInt(products.length);
log.info(&quot;random product is:&quot;+products[random]);
for(int i=0;i&lt;products.length;i++){       
	products[i]=products[i].toUpperCase();       
	if(products[i].equalsIgnoreCase(&quot;AirCooler&quot;))products[i]=&quot;AirConditioner&quot;;  
	//products[i]=products[i].toUpperCase();
	log.info(products[i]);
}
boolean validate=${__groovy(&quot;This is My Test String&quot;.contains(&quot;My&quot;),)};
if(validate)log.info(&quot;MY&quot;);
else log.info(&quot;MY is not present&quot;);
log.info(&quot;value:&quot;+validate);
String originalString=&quot;Performance&quot;;
String encodedString=Base64.getEncoder().encodeToString(originalString.getBytes());
log.info(&quot;Encoded: &quot;+encodedString);
byte[]decodedBytes=Base64.getDecoder().decode(encodedString);
String decodedString=new String(decodedBytes);
log.info(&quot;Decoded: &quot;+decodedString);</stringProp>
        </BeanShellPostProcessor>
